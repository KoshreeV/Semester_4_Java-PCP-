import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int x=sc.nextInt();
	    int sum=0;
	   
	    int[] arr=new int[x];
	    for (int i=0;i<x;i++){
	        arr[i]=sc.nextInt();
	    }
	    for (int i=0;i<x;i+=2){
	        sum+=arr[i];
	    }
	     
	    System.out.println(sum);
	    
	    
		
	}
}
